<div class="container">
<h1>Create your Bookmark</h1>
<%= simple_form_for [ @list, @bookmark ] do |f| %>
  <%= f.input :comment %>
  <%= f.association :movie %>
  <%= f.submit "Confirm", class: "btn btn-primary"%>
  <%= link_to "back", list_path(@list), class: "btn btn-secondary" %>
<% end %>
</div>
